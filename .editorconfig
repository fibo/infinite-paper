# EditorConfig is awesome: http://EditorConfig.org

# Stop the editor from looking for .editorconfig files in the parent directories.
root = true

[*]

charset = utf-8
insert_final_newline = true
trim_trailing_whitespace = true

indent_style = space
indent_size = 2

[infinite-paper.js]
end_of_line = lf
indent_style = tab
indent_size = 4

